<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="500dip" />
            <stroke android:width="1dip" android:color="#dc6193b7" />
            <gradient android:angle="-90" android:startColor="#e7344b59" android:endColor="#689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="500dip" />
            <stroke android:width="1dip" android:color="#dc6193b7" />
            <solid android:color="#58857e"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="500dip" />
            <stroke android:width="1dip" android:color="#dc6193b7" />
            <gradient android:angle="-90" android:startColor="#8da1ba" android:endColor="#ed587585" />
        </shape>
    </item>
</selector>